{
    "compilerOptions": {
        "target": "ES2022",
        "module": "ES2022",
        "moduleResolution": "bundler",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "strict": true,
        "allowImportingTsExtensions": true,
        "noEmit": true
    },
    "exclude": [
        "node_modules"
    ]
}